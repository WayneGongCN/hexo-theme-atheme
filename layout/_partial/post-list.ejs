<% if (posts && posts.length) { %>
  <ul class="container post-list">
    <% posts.forEach(function (post) { %>
      <li class="lh-lg row ">
        <time class="col-sm-auto font-monospace em-6"><%= date(post.date, "YYYY-MM-DD") %></time>
        <a class="col-sm-auto text-truncate text-reset " href="<%- url_for(post.path) %>"><%= post.title %></a>
      </li>
    <% }) %>
  </ul>
<% } %>